{
  "dataset": {
    "root": "data/ham10000",
    "image_size": 224,
    "split_seed": 42,
    "train_frac": 0.8,
    "val_frac": 0.1,
    "test_frac": 0.1
  },
  "train": {
    "seed": 1,
    "epochs": 10,
    "batch_size": 32,
    "lr": 0.0003,
    "weight_decay": 0.0001,
    "num_workers": 4,
    "use_class_weights": true
  },
  "model": {
    "backbone": "efficientnet_b0",
    "pretrained": true,
    "dropout": 0.2
  }
}
